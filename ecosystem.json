{
  "apps": [
    {
      "name": "sync-to-ical",
      "script": "index.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "log_file": "../logs/sync-to-ical.log",
      "out_file": "/dev/null",
      "error_file": "/dev/null",
      "logrotate": {
        "max_size": "3M",
        "retain": 5,
        "compress": false,
        "dateFormat": "MMDDHHmmss"
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
